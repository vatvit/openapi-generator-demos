{
    "name": "openapi-generator/laravel-max-integration-tests",
    "description": "Integration tests for laravel-max generator",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "TictactoeApi\\Model\\": "../../generated/laravel-max/tictactoe/app/Models/",
            "TictactoeApi\\Api\\Http\\": "../../generated/laravel-max/tictactoe/app/Http/",
            "TictactoeApi\\Api\\Handlers\\": "../../generated/laravel-max/tictactoe/app/Handlers/",
            "TictactoeApi\\Api\\Security\\": "../../generated/laravel-max/tictactoe/app/Security/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "find ../../generated/laravel-max/tictactoe -name '*.php' -exec php -l {} \\;",
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
