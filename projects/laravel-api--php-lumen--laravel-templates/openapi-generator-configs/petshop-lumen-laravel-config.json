{
  "invokerPackage": "PetStoreApiV2\\Server",
  "modelPackage": "Models",
  "apiPackage": "Api",
  "srcBasePath": "lib",
  "variableNamingConvention": "camelCase",
  "gitRepoId": "petstore-lumen-laravel",
  "gitUserId": "vatvit",
  "artifactVersion": "1.0.0",
  "packageName": "PetStoreApiV2LumenLaravel",
  "composerVendorName": "vatvit",
  "composerPackageName": "petstore-api-lumen-laravel",
  "licenseName": "MIT",
  "files": {
    "api_controller.mustache": {
      "folder": "lib/Http/Controllers",
      "destinationFilename": "{{{classname}}}Controller.php",
      "templateType": "API"
    },
    "api_form_request.mustache": {
      "folder": "lib/Http/Requests",
      "destinationFilename": "{{{classname}}}Request.php",
      "templateType": "API"
    },
    "api_resource.mustache": {
      "folder": "lib/Http/Resources",
      "destinationFilename": "{{{classname}}}Resource.php",
      "templateType": "Model"
    },
    "api_response_factory.mustache": {
      "folder": "lib/Http/Responses",
      "destinationFilename": "{{{classname}}}ResponseFactory.php",
      "templateType": "API"
    },
    "security_interface.mustache": {
      "folder": "lib/Security",
      "destinationFilename": "SecurityInterface.php",
      "templateType": "SupportingFiles"
    },
    "security_stub.mustache": {
      "folder": "lib/Security",
      "destinationFilename": "SecurityMiddleware.php",
      "templateType": "SupportingFiles"
    },
    "security_validator.mustache": {
      "folder": "lib/Security",
      "destinationFilename": "SecurityValidator.php",
      "templateType": "SupportingFiles"
    }
  }
}
