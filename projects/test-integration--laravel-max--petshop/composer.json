{
    "name": "openapi-generator/laravel-max-petshop-test",
    "description": "Integration test for laravel-max generator with petshop spec",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "TictactoeApi\\Model\\": "../../generated/laravel-max/petshop/app/Models/",
            "TictactoeApi\\Api\\Http\\": "../../generated/laravel-max/petshop/app/Http/",
            "TictactoeApi\\Api\\Handlers\\": "../../generated/laravel-max/petshop/app/Handlers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "find ../../generated/laravel-max/petshop -name '*.php' -exec php -l {} \\;",
        "test": [
            "@lint",
            "phpunit"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
